<!doctype html>
<html class="md-perspective">
<head>
	<meta name="robots" content="noindex">
	<title>QRPass</title>
	<link rel="stylesheet" type="text/css" href="css/style.css">
</head>
<body>

	<div class="md-modal md-effect" id="modal-1">
		<div class="md-content">
			<button class="md-close" onclick="hideModal()">&#xea0f;</button>
			<button id="copy-button-userName">Copy username to Clipboard</button><br>
			<button id="copy-button-password">Copy password to Clipboard</button><br>
		</div>
	</div>
	<div class="md-overlay"></div>

	<div class="container" id="container">
		<div class="qrcode-wrap">
			<div class="qrcode qrcode-hide" id="qrcode">
				<a href="apk/android.apk" class="icon-android">&#xeac1;</a>
			</div>
		</div>
	</div>

	<script type="text/javascript" src="/scripts/aes.js"></script>
	<script type="text/javascript" src="/scripts/enc-base64.js"></script>
	<script type="text/javascript" src="/scripts/pbkdf2.js"></script>
	<script type="text/javascript" src="/scripts/qrcode.min.js"></script>
	<script src="/socket.io/socket.io.js"></script>
	<script type="text/javascript" src="/ZeroClipboard.js"></script>
	<script type="text/javascript" src="scripts/qrpass.js"></script>
</body>
</html>
